generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Process {
  id           String   @id @default(uuid())
  type         String
  status       String
  currentStep  String?
  assignedRole String
  data         Json
  history      Json
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
