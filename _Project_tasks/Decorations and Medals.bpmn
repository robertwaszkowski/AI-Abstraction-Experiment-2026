  <bpmn:collaboration id="Collaboration_DPE_1_6">
    <bpmn:participant id="Participant_Pool_WAT" name="Military University of Technology (Decorations and Medals DPE/1-6)" processRef="Process_DPE_1_6" />
  </bpmn:collaboration>
  <bpmn:process id="Process_DPE_1_6" isExecutable="true" camunda:versionTag="1.0">
    <bpmn:extensionElements />
    <bpmn:laneSet id="LaneSet_DPE_1_6">
      <bpmn:lane id="Lane_HeadOU" name="Head of O.U.">
        <bpmn:flowNodeRef>StartEvent_ProcessInitiated</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SubmitApplication</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_PD" name="Personnel Department (PD)">
        <bpmn:flowNodeRef>Task_PresentApplicationsToRKR</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EnterToRegister</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ForwardApplicationsToMPD</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_RKRDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Rejected</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Completed</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PresentApplicationsForAcceptance</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReceiveDecision</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_PRK_Chancellor" name="PRK / Chancellor">
        <bpmn:flowNodeRef>Task_ReviewApplications</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_RKR" name="Rector (RKR)">
        <bpmn:flowNodeRef>Task_MakeDecision</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_MPD" name="Military Personnel Department (MPD)">
        <bpmn:flowNodeRef>Task_HandleApplicationsExternal</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_ProcessInitiated" name="Process initiated">
      <bpmn:outgoing>Flow_StartToSubmit</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_StartToSubmit" sourceRef="StartEvent_ProcessInitiated" targetRef="Task_SubmitApplication" />
    <bpmn:sequenceFlow id="Flow_PresentAcceptanceToReview" sourceRef="Task_PresentApplicationsForAcceptance" targetRef="Task_ReviewApplications" />
    <bpmn:sequenceFlow id="Flow_ReviewToPresentRKR" sourceRef="Task_ReviewApplications" targetRef="Task_PresentApplicationsToRKR" />
    <bpmn:sequenceFlow id="Flow_PresentRKRToMakeDecision" sourceRef="Task_PresentApplicationsToRKR" targetRef="Task_MakeDecision" />
    <bpmn:sequenceFlow id="Flow_MakeDecisionToRKRDecision" sourceRef="Task_MakeDecision" targetRef="Gateway_RKRDecision" />
    <bpmn:sequenceFlow id="Flow_Accepted" name="Yes" sourceRef="Gateway_RKRDecision" targetRef="Task_ForwardApplicationsToMPD" />
    <bpmn:sequenceFlow id="Flow_Rejected" name="No" sourceRef="Gateway_RKRDecision" targetRef="EndEvent_Rejected" />
    <bpmn:sequenceFlow id="Flow_ForwardToHandleExternal" sourceRef="Task_ForwardApplicationsToMPD" targetRef="Task_HandleApplicationsExternal" />
    <bpmn:sequenceFlow id="Flow_HandleExternalToReceiveDecision" sourceRef="Task_HandleApplicationsExternal" targetRef="Task_ReceiveDecision" />
    <bpmn:sequenceFlow id="Flow_ReceiveDecisionToEnterRegister" sourceRef="Task_ReceiveDecision" targetRef="Task_EnterToRegister" />
    <bpmn:sequenceFlow id="Flow_EnterRegisterToCompleted" sourceRef="Task_EnterToRegister" targetRef="EndEvent_Completed" />
    <bpmn:sequenceFlow id="Flow_SubmitToPresentAcceptance" sourceRef="Task_SubmitApplication" targetRef="Task_PresentApplicationsForAcceptance" />
    <bpmn:userTask id="Task_MakeDecision" name="Make decision" aurea:responsibleRef="Lane_RKR">
      <bpmn:incoming>Flow_PresentRKRToMakeDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_MakeDecisionToRKRDecision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_HandleApplicationsExternal" name="Handle applications (external transfer)" aurea:responsibleRef="Lane_MPD">
      <bpmn:incoming>Flow_ForwardToHandleExternal</bpmn:incoming>
      <bpmn:outgoing>Flow_HandleExternalToReceiveDecision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_SubmitApplication" name="Submit application for distinction" aurea:responsibleRef="Lane_HeadOU">
      <bpmn:incoming>Flow_StartToSubmit</bpmn:incoming>
      <bpmn:outgoing>Flow_SubmitToPresentAcceptance</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ReviewApplications" name="Review applications and forward to PD" aurea:responsibleRef="Lane_PRK_Chancellor">
      <bpmn:incoming>Flow_PresentAcceptanceToReview</bpmn:incoming>
      <bpmn:outgoing>Flow_ReviewToPresentRKR</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_PresentApplicationsToRKR" name="Present reviewed applications to Rector" aurea:responsibleRef="Lane_PD">
      <bpmn:incoming>Flow_ReviewToPresentRKR</bpmn:incoming>
      <bpmn:outgoing>Flow_PresentRKRToMakeDecision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_EnterToRegister" name="Enter decoration into register" aurea:responsibleRef="Lane_PD">
      <bpmn:incoming>Flow_ReceiveDecisionToEnterRegister</bpmn:incoming>
      <bpmn:outgoing>Flow_EnterRegisterToCompleted</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ForwardApplicationsToMPD" name="Forward accepted applications to MPD" aurea:responsibleRef="Lane_PD">
      <bpmn:incoming>Flow_Accepted</bpmn:incoming>
      <bpmn:outgoing>Flow_ForwardToHandleExternal</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_RKRDecision" name="Application accepted?">
      <bpmn:incoming>Flow_MakeDecisionToRKRDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_Accepted</bpmn:outgoing>
      <bpmn:outgoing>Flow_Rejected</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_Rejected" name="Application rejected">
      <bpmn:incoming>Flow_Rejected</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_Completed" name="Process ended">
      <bpmn:incoming>Flow_EnterRegisterToCompleted</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:userTask id="Task_PresentApplicationsForAcceptance" name="Present applications for acceptance (PRK/Chancellor)" aurea:responsibleRef="Lane_PD">
      <bpmn:incoming>Flow_SubmitToPresentAcceptance</bpmn:incoming>
      <bpmn:outgoing>Flow_PresentAcceptanceToReview</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:task id="Task_ReceiveDecision" name="Receive decision on award" aurea:responsibleRef="Lane_PD">
      <bpmn:incoming>Flow_HandleExternalToReceiveDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_ReceiveDecisionToEnterRegister</bpmn:outgoing>
    </bpmn:task>
  </bpmn:process>
